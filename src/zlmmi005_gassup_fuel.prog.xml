     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>ZLMMI005_GASSUP_FUEL</TDNAME>
      <TDID>RE</TDID>
      <TDSPRAS>E</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
